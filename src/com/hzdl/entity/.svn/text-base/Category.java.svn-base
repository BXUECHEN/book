package com.hzdl.entity;

public class Category {
	
	public int CID;
	public String CName;
	
	public int getCID() {
		return CID;
	}
	public void setCID(int cID) {
		CID = cID;
	}
	public String getCName() {
		return CName;
	}
	public void setCName(String cName) {
		CName = cName;
	}
	
	@Override
	public String toString() {
		return "category [CID=" + CID + ", CName=" + CName + "]";
	}
	
	
	
	

}
